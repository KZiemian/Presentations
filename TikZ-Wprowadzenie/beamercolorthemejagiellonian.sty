\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthemejagiellonian}[2017/01/23 Jagiellonian color theme]
\RequirePackage{pgfopts}
\pgfkeys{
  /jagiellonian/color/block/.cd,
  .is choice,
  transparent/.code=\jagiellonian@block@transparent,
  fill/.code=\jagiellonian@block@fill,
}
\pgfkeys{
  /jagiellonian/color/style/.cd,
  .is choice,
  light/.code=\jagiellonian@colors@light,
    dark/.code=\jagiellonian@colors@dark,
}
\pgfkeys{
  /jagiellonian/color/titlefontcolor/.cd,
  .is choice,
  light/.code=\jagiellonian@colors@titlefontcolorlight,
  dark/.code=\jagiellonian@colors@titlefontcolordark,
}
\pgfkeys{
  /jagiellonian/color/titlebackgroundcolor/.cd,
  .is choice,
  light/.code=\jagiellonian@colors@titlebackgroundcolorlight,
  dark/.code=\jagiellonian@colors@titlebackgroundcolordark,
}
\newcommand{\jagiellonian@color@setdefaults}{
  \pgfkeys{/jagiellonian/color/.cd,
    style=light,
    block=transparent,
  }
}
\definecolor{JagiellonianBlue}{HTML}{005BA6}

\definecolor{jNormalTextForegroundGrey}{HTML}{7E7E7E}
\definecolor{jMathTextForegroundGrey}{HTML}{4E4E4E}
\definecolor{jNormalMathTextBackgroundLight}{HTML}{FAFAFA}

\definecolor{jNormalTextForegroundBlueGrey}{HTML}{818F9B}
\definecolor{jMathTextForegroundWhite}{HTML}{D6DEE5}
\definecolor{jNormalMathTextBackgroundDark}{HTML}{023159}

\definecolor{jAxisRed}{HTML}{A21C0E}
\definecolor{jAxisGreen}{HTML}{7FAA60}
\definecolor{jAxisBlue}{HTML}{0089FF}

\definecolor{jStrongWhite}{HTML}{FBFBFB}
\definecolor{jGrey}{HTML}{8F8E8E}
\definecolor{jGray}{HTML}{8F8E8E}

\definecolor{jViolet}{HTML}{8E58CB}
\definecolor{jLightGreen}{HTML}{14B03D}
\definecolor{jDarkOrange}{HTML}{EB811B}
\definecolor{jLightBrown}{HTML}{EB811B}

\newcommand{\TextFGColor}{jNormalTextForegroundGrey}
\newcommand{\NormalTextBGColor}{jNormalMathTextBackgroundLight}
\newcommand{\MathFGColor}{jMathTextForegroundGrey}
\newcommand{\jagiellonian@colors@light}{
  \jagiellonian@colors@maintextdarkonlight
  %
  %
  \setbeamercolor{titlelike}{use=normal text, parent=normal text}
  \setbeamercolor{author}{use=normal text, parent=normal text}
  \setbeamercolor{date}{use=normal text, parent=normal text}
  \setbeamercolor{institute}{use=normal text, parent=normal text}
}
\newcommand{\jagiellonian@colors@dark}{
  \jagiellonian@colors@maintextlightondark
  %
  \usebeamercolor[fg]{normal text}
  %
  % black!2 is now jNormalMathTextBackgroundLight
  % \setbeamercolor{titlelike}{use=normal text, fg=black!2,
  %   bg=normal text.bg}
  % \setbeamercolor{author}{use=normal text, fg=black!2,
  %   bg=normal text.bg}
  % \setbeamercolor{date}{use=normal text, fg=black!2,
  %   bg=normal text.bg}
  % \setbeamercolor{institute}{use=normal text, fg=black!2,
  % bg=normal text.bg}
  \setbeamercolor{titlelike}{fg=jNormalMathTextBackgroundLight,bg=JagiellonianBlue}
  \setbeamercolor{author}{fg=jNormalMathTextBackgroundLight,bg=JagiellonianBlue}
  \setbeamercolor{date}{fg=jNormalMathTextBackgroundLight,bg=JagiellonianBlue}
  \setbeamercolor{institute}{fg=jNormalMathTextBackgroundLight,bg=JagiellonianBlue}
}
\setbeamercolor{alerted text}{%
  fg=jDarkOrange
}
\setbeamercolor{example text}{%
  fg=jLightGreen
}
\newcommand{\jagiellonian@colors@maintextlightondark}{
  \setbeamercolor{normal text}{%
    fg=jNormalTextForegroundBlueGrey,
    bg=jNormalMathTextBackgroundDark,
  }
  %
  %
  \setbeamercolor{math text}{
    use=normal text,
    fg=jMathTextForegroundWhite,
    bg=normal text.bg,
  }
  %
  %
  \renewcommand{\TextFGColor}{jNormalTextForegroundBlueGrey}
  \renewcommand{\NormalTextBGColor}{jNormalMathTextBackgroundDark}
  %
  \renewcommand{\MathFGColor}{jMathTextForegroundWhite}
}
\newcommand{\jagiellonian@colors@maintextdarkonlight}{
  \setbeamercolor{normal text}{%
    fg=jNormalTextForegroundGrey,
    bg=jNormalMathTextBackgroundLight,
  }
  %
  %
  \setbeamercolor{math text}{
    use=normal text,
    fg=jMathTextForegroundGrey,
    bg=normal text.bg,
  }
  %
  %
  \renewcommand{\TextFGColor}{jNormalTextForegroundGrey}
  \renewcommand{\NormalTextBGColor}{jNormalMathTextBackgroundLight}
  %
  \renewcommand{\MathFGColor}{jMathTextForegroundGrey}
}
\newcommand{\jagiellonian@colors@titlefontcolorlight}{
  \setbeamercolor{titlelike}{fg=jNormalMathTextBackgroundLight}
  \setbeamercolor{author}{fg=jNormalMathTextBackgroundLight}
  \setbeamercolor{date}{fg=jNormalMathTextBackgroundLight}
  \setbeamercolor{institute}{fg=jNormalMathTextBackgroundLight}
}
\newcommand{\jagiellonian@colors@titlefontcolordark}{
  \setbeamercolor{titlelike}{fg=jNormalTextForegroundGrey}
  \setbeamercolor{author}{fg=jNormalTextForegroundGrey}
  \setbeamercolor{date}{fg=jNormalTextForegroundGrey}
  \setbeamercolor{institute}{fg=jNormalTextForegroundGrey}
}
\newcommand{\jagiellonian@colors@titlebackgroundcolorlight}{
  \setbeamercolor{titlelike}{bg=jNormalMathTextBackgroundLight}
  \setbeamercolor{author}{bg=jNormalMathTextBackgroundLight}
  \setbeamercolor{date}{bg=jNormalMathTextBackgroundLight}
  \setbeamercolor{institute}{bg=jNormalMathTextBackgroundLight}
}
\newcommand{\jagiellonian@colors@titlebackgroundcolordark}{
  \setbeamercolor{titlelike}{bg=jNormalTextBackgroundDark}
  \setbeamercolor{author}{bg=jNormalTextBackgroundDark}
  \setbeamercolor{date}{bg=jNormalTextBackgroundDark}
  \setbeamercolor{institute}{bg=jNormalTextBackgroundDark}
}
\setbeamercolor{structure}{use=normal text, fg=normal text.fg}
\setbeamercolor{palette primary}{
  fg=jStrongWhite,
  bg=JagiellonianBlue,
}
\setbeamercolor{frametitle}{%
  fg=jNormalMathTextBackgroundLight,
  bg=JagiellonianBlue,
}
\setbeamercolor{progress bar}{%
  use=alerted text,
  fg=alerted text.fg,
  bg=alerted text.fg!50!black!30
}
\setbeamercolor{title separator}{
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in head/foot}{%
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in section page}{
  use=progress bar,
  parent=progress bar
}
\newcommand{\jagiellonian@block@transparent}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=
  }
  \setbeamercolor{block body}{
    bg=
  }
}
\newcommand{\jagiellonian@block@fill}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=normal text.bg!80!fg
  }
  %
  %
  \setbeamercolor{block body}{
    use={block title, normal text},
    bg=block title.bg!50!normal text.bg
  }
}
\setbeamercolor{block title alerted}{%
  use={block title, alerted text},
  bg=block title.bg,
  fg=alerted text.fg
}
\setbeamercolor{block title example}{%
  use={block title, example text},
  bg=block title.bg,
  fg=example text.fg
}
\setbeamercolor{block body alerted}{use=block body, parent=block body}
\setbeamercolor{block body example}{use=block body, parent=block body}
\setbeamercolor{footnote}{fg=normal text.fg!90}
\setbeamercolor{footnote mark}{fg=.}
\setbeamercolor{bibliography entry author}{fg=, bg=}
\setbeamercolor{bibliography entry title}{fg=, bg=}
\setbeamercolor{bibliography entry location}{fg=, bg=}
\setbeamercolor{bibliography entry note}{fg=, bg=}
\jagiellonian@color@setdefaults
\ProcessPgfPackageOptions{/jagiellonian/color}
